<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.AjouterUserController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="tf_nom" layoutX="240.0" layoutY="60.0" promptText="Nom" />
        <TextField fx:id="tf_prenom" layoutX="240.0" layoutY="110.0" promptText="Prénom" />
        <TextField fx:id="tf_email" layoutX="240.0" layoutY="160.0" promptText="Email" />
        <TextField fx:id="tf_password" layoutX="240.0" layoutY="210.0" promptText="Mot de passe" />
        <TextField fx:id="tf_role" layoutX="240.0" layoutY="260.0" promptText="Rôle" />
        <TextField fx:id="tf_tel" layoutX="240.0" layoutY="310.0" promptText="Téléphone" />
        <TextField fx:id="tf_adresse" layoutX="240.0" layoutY="360.0" promptText="Adresse" />
        <DatePicker fx:id="dp_signUpDate" layoutX="240.0" layoutY="410.0" promptText="Date d'inscription" />
        <Button layoutX="240.0" layoutY="460.0" mnemonicParsing="false" onAction="#ajouter" text="Ajouter" />
        <Button layoutX="340.0" layoutY="460.0" mnemonicParsing="false" onAction="#annuler" text="Annuler" />
    </children>
</AnchorPane>
